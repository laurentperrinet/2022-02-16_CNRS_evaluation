	Doi = {10.3389/fninf.2013.00014},
	Issn = {1662-5196},
	Journal = {Frontiers in Neuroinformatics},
	Keywords = {bayesian, drift-diffusion, hierarchical\_model, python},
	Pmcid = {PMC3731670},
	Pmid = {23935581},
	Posted-At = {2014-01-14 13:55:13},
	Priority = {2},
	Title = {{HDDM}: Hierarchical Bayesian estimation of the {Drift-Diffusion} Model in Python},
	Url = {http://dx.doi.org/10.3389/fninf.2013.00014},
