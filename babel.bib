	Citeulike-Linkout-0 = {http://hal.inria.fr/inria-00492001/en/},
	Institution = {INRIA},
	Keywords = {benchmark, models, motion, neural, perception, perrinet12pred, smooth\_pursuit},
	Month = jun,
	Number = {RR-7317},
	Pages = {18},
	Posted-At = {2012-01-09 13:04:16},
	Priority = {2},
	Title = {Towards a bio-inspired evaluation methodology for motion estimation models},
	Url = {http://hal.inria.fr/inria-00492001/en/},
