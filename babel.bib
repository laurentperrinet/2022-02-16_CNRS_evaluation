	Doi = {10.3389/fncom.2012.00041},
	Issn = {1662-5188},
	Journal = {Frontiers in Computational Neuroscience},
	Keywords = {dynamical\_model, kaplan13, lateral\_connections, patch-based, recurrent\_neural\_networks},
	Posted-At = {2012-07-10 13:45:56},
