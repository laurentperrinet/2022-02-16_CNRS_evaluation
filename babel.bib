	Posted-At = {2011-01-26 17:52:09},
	Priority = {0},
	Title = {Distributed occlusion reasoning for tracking with nonparametric belief propagation},
	Url = {http://citeseerx.ist.psu.edu/viewdoc/summary?doi=10.1.1.68.9245},
