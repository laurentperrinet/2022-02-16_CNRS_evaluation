	Posted-At = {2014-02-21 15:14:58},
	Priority = {4},
	Publisher = {Public Library of Science},
	Title = {A Unified Model of Heading and Path Perception in Primate {MSTd}},
	Url = {http://dx.doi.org/10.1371/journal.pcbi.1003476},
	Volume = {10},
	Year = {2014},
