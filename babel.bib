	Priority = {2},
	Publisher = {Springer Berlin Heidelberg},
	Title = {Hebbian-based mean shift for learning the diverse shapes of V1 simple cell receptive fields},
	Url = {http://dx.doi.org/10.1007/s11434-013-0041-4},
