	Citeulike-Linkout-2 = {http://www.perceptionweb.com/abstract.cgi?id=p3158},
	Comment = {* would you see the illusion in Fig 8 if it is actually moving?},
	Date-Modified = {2014-04-09 08:02:58 +0000},
	Doi = {10.1068/p3158},
	Issn = {1468-4233},
	Journal = {Perception},
	Keywords = {freemove, space-time\_representation, time},
	Number = {2},
	Pages = {195--208},
	Posted-At = {2014-01-15 09:40:30},
	Priority = {0},
	Publisher = {Pion Ltd},
	Title = {'Perceiving the present' as a framework for ecological explanations of the misperception of projected angle and angular size},
