	Issn = {0169-1015},
	Journal = {Spatial vision},
	Keywords = {multiple-dots, perrinet12pred, predictive-coding},
	Number = {3},
	Pages = {179--197},
	Pmid = {3153671},
	Posted-At = {2011-11-24 16:20:56},
	Priority = {3},
