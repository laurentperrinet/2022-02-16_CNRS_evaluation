	Priority = {2},
	Title = {Narrow versus wide tuning curves: what's best for a population code?},
	Url = {http://www.gatsby.ucl.ac.uk/\~{}pel/publications.html},
	Volume = {11},
	Year = {1999},
	Bdsk-Url-1 = {http://www.gatsby.ucl.ac.uk/%5C~%7B%7Dpel/publications.html}}

@misc{Deneve03,
	Abstract = {It is widely believed that individual cortical neurons are unreliable encoder and that we need to consider population of cells to perform reliable computations. But it has been suggested that the observed neural correlations limit the capacity for averaging out the neural noise. Moreover, single sensory neurons have been reported with discrimination threshold close or even lower than the animal's, and many correlate strongly with the animal subjective perception or behavioral choice. We propose, on the other hand, that neural correlations and high choice probability are no hindrance but the consequence of efficient information processing in population of neurons. We have built a general model for how optimal integration could be performed in recurrent cortical networks. We propose that these networks implement an internal model of their sensory inputs and motor outputs by predicting iteratively population encoded sensory and motor variables from each other. These predictions carried in time are combined on-line with the sensory input to give the most reliable estimates of the sensory and motor states. In particular, we predict that the initially local neural information is progressively shared by all cortical neurons, leading to an increase in neural correlations and choice probability during integration. In this way, all areas participate in integration, inference and decision making. We will show that this model accounts for a wide range of neural data and makes specific testable predictions, in particular a link between the observed choice probability, neural correlations, and the requirement of the task. We will present a generalisation of this approach where cortical networks are equivalent to bayesian networks propagating estimates and certainties. Finally, we will relate the structure of cortical pathways and modularity to the type of internal statistical model they might implement.},
	Author = {Deneve, Sophie and Pouget, Alexandre},
	Citeulike-Article-Id = {8904412},
	Citeulike-Linkout-0 = {http://sfn.scholarone.com/itin2003/index.html},
	Howpublished = {Poster},
	Keywords = {bayesian, belief\_propagation, neural-representation, recurrent\_neural\_networks},
	Organization = {Gatsby Unit, UCL},
	Posted-At = {2011-07-11 13:10:46},
	Priority = {2},
	Title = {Optimal Integration by Belief Propagation in Recurrent Cortical Networks},
	Url = {http://sfn.scholarone.com/itin2003/index.html},
	Year = {2003},
	Bdsk-Url-1 = {http://sfn.scholarone.com/itin2003/index.html}}

@article{Ma08,
	Abstract = {Systems neuroscience traditionally conceptualizes a population of spiking neurons as merely encoding the value of a stimulus. Yet, psychophysics has revealed that people take into account stimulus uncertainty when performing sensory or motor computations and do so in a nearly Bayes-optimal way. This suggests that neural populations do not encode just a single value but an entire probability distribution over the stimulus. Several such probabilistic codes have been proposed, including one that utilizes the structure of neural variability to enable simple neural implementations of probabilistic computations such as optimal cue integration. This approach provides a quantitative link between Bayes-optimal behaviors and specific neural operations. It allows for novel ways to evaluate probabilistic codes and for predictions for physiological population recordings.},
