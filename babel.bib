	Keywords = {sparse\_coding},
	Month = jun,
	Posted-At = {2011-10-17 08:47:14},
	Priority = {2},
	Title = {Multitask dictionary learning and sparse representation based single-image super-resolution reconstruction},
	Url = {http://dx.doi.org/10.1016/j.neucom.2011.04.014},
