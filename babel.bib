	Citeulike-Linkout-3 = {http://www.hubmed.org/display.cgi?uids=24198798},
	Doi = {10.3389/fpsyg.2013.00794},
	Issn = {1664-1078},
	Journal = {Frontiers in Psychology},
	Keywords = {freemove, time},
	Pmcid = {PMC3814518},
	Pmid = {24198798},
	Posted-At = {2014-01-15 12:43:25},
	Priority = {2},
	Title = {Spatial warping by oriented line detectors can counteract neural delays},
	Url = {http://dx.doi.org/10.3389/fpsyg.2013.00794},
	Volume = {4},
	Year = {2013},
	Bdsk-Url-1 = {http://dx.doi.org/10.3389/fpsyg.2013.00794}}

@article{Changizi02,
	Abstract = {There is a significant delay between the time when light hits the retina and the time of the consequent percept. It has been hypothesized that the visual system attempts to correct for this latency by generating a percept representative of the way the world probably is at the time the percept is elicited, rather than a percept of the recent past. Here we show that such a 'perceiving the present' hypothesis explains a number of classical geometrical illusions: the Hering, Orbison, {M\"{u}ller-Lyer}, Double Judd, Poggendorff, Corner, and {Upside-down-T} illusions. Each stimulus is perceived as it would project in the next moment were the observer moving through the scene the stimulus probably represents. We also examine one general class of predictions made by the hypothesis, and report psychophysical experiments confirming the predictions.},
	Author = {Changizi, Mark A. and Widders, David M.},
	Citeulike-Article-Id = {12921616},
	Citeulike-Linkout-0 = {http://dx.doi.org/10.1068/p3412},
	Citeulike-Linkout-1 = {http://view.ncbi.nlm.nih.gov/pubmed/12430950},
	Citeulike-Linkout-2 = {http://www.hubmed.org/display.cgi?uids=12430950},
	Doi = {10.1068/p3412},
	Issn = {1468-4233},
	Journal = {Perception},
	Keywords = {freemove, time, visual\_illusion},
	Number = {10},
	Pages = {1241--1262},
	Pmid = {12430950},
	Posted-At = {2014-01-15 11:37:44},
	Priority = {4},
	Title = {Latency correction explains the classical geometrical illusions},
