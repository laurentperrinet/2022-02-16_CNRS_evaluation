	Priority = {0},
	Title = {Complex local phase based subjective Surfaces ({CLAPSS}) and its application to {DIC} red Blood cell image segmentation},
	Url = {http://dx.doi.org/10.1016/j.neucom.2012.06.015},
	Year = {2012},
