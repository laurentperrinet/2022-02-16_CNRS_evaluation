	Issn = {01651684},
	Journal = {Signal Processing},
	Keywords = {multi-scale, wavelets},
	Month = dec,
	Number = {12},
	Pages = {2699--2730},
	Posted-At = {2011-12-02 17:04:30},
	Priority = {2},
	Title = {A panorama on multiscale geometric representations, intertwining spatial, directional and frequency selectivity},
	Url = {http://dx.doi.org/10.1016/j.sigpro.2011.04.025},
	Volume = {91},
	Year = {2011},
