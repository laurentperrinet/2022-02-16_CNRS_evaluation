@article{Friston10c,
	Abstract = {Chris Thornton [1] poses some simple but key questions about the free-energy principle reviewed in [2] . These puzzles have simple and clear answers: Puzzle: '' A generative model of causal structure in the environment is [then] obtained, on which basis the agent is able to infer the 'causes of sensory samples' [ibid. p. 294]. What is unclear is how this mechanism would function where sensory samples are ambiguous'' [1] . Answer: One of the main motivations for the free-energy principle is its appeal to [approximate] Bayesian inference where ambiguities are resolved by priors [3] . Priors are mandated by the (ill-posed) problems created by ambiguity and empirical priors are an integral part of hierarchical inference [{2,Box} 3]. This is not theoretical hand waving; in biophysics, the free-energy formulation is used routinely to solve difficult ill-posed inverse problems (e.g. [4] ). Puzzle: '' On the face of it, no particular stand is taken on emergence of the structures that mediate minimization. But looking at the definition of free-energy, we find a significant role being played by the variable . It is values of this variable that encapsulate the brain's representation of 'environmental causes''' [1] . Answer: The representations are not environmental causes but the sufficient statistics Î¼ of the brain's recognition density q ( ; Î¼ ); these include synaptic activity and efficacy [2] . The implicit optimization of neuronal connections (i.e. perceptual learning) leads to hierarchical brain structures (models) that recapitulate causal structure in the sensorium. This optimization process can 'prune' the form or structure of the model (cf., synaptic pruning [5] ) and is used routinely in model optimization (e.g. automatic relevance determination [6] ). Furthermore, one could regard natural selection as optimizing the structural form of models at an evolutionary scale, through minimizing free-energy (where it is called free-fitness [7] ). In a statistical setting, free-energy bounds on model evidence are used routinely in Bayesian model selection (where the log model evidence is negative surprise, e.g. [8] ;) ( Figure 1 ).},
	Author = {Friston, Karl},
	Citeulike-Article-Id = {6479493},
	Citeulike-Linkout-0 = {http://dx.doi.org/10.1016/j.tics.2009.11.008},
	Day = {22},
	Doi = {10.1016/j.tics.2009.11.008},
	Issn = {13646613},
	Journal = {Trends in Cognitive Sciences},
	Keywords = {free\_energy},
	Month = feb,
	Number = {2},
	Pages = {54--55},
	Posted-At = {2011-06-16 09:29:46},
	Priority = {2},
	Title = {Some free-energy puzzles resolved: response to Thornton},
	Url = {http://dx.doi.org/10.1016/j.tics.2009.11.008},
	Volume = {14},
	Year = {2010},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.tics.2009.11.008}}

