	Priority = {3},
	Publisher = {Public Library of Science},
	Title = {The Correlation Structure of Local Neuronal Networks Intrinsically Results from Recurrent Dynamics},
	Url = {http://dx.doi.org/10.1371/journal.pcbi.1003428},
