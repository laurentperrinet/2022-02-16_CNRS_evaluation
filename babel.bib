@article{Purves96,
	Abstract = {Wheels turning in the movies or in other forms of stroboscopic presentation often appear to be rotating backward. Remarkably, a similar illusion is also seen in continuous light. The occurrence of this perception in the absence of intermittent illumination suggests that we normally see motion, as in movies, by processing a series of visual episodes.},
