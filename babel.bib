	Issn = {1534-7362},
	Journal = {Journal of Vision},
	Keywords = {kaplan13, traveling\_wave},
	Month = dec,
	Number = {13},
	Pages = {25},
	Posted-At = {2013-04-18 13:09:03},
	Priority = {3},
	Publisher = {Association for Research in Vision and Ophthalmology},
	Title = {Standing waves and traveling waves in visual cortex},
	Url = {http://dx.doi.org/10.1167/6.13.25},
