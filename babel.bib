	Doi = {10.3389/fncom.2013.00010},
	Issn = {1662-5188},
	Journal = {Frontiers in computational neuroscience},
	Keywords = {assofield, orientation\_selectivity, self\_organization, spikes},
	Pmcid = {PMC3583036},
